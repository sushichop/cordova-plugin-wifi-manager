<?xml version='1.0' encoding='utf-8'?>
<widget id="net.sushichop.cordovasamples.wifimanagerpluginsampleapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>wifi-manager-plugin-sample-app</name>
    <description>Wi-Fi Manager Plugin Sample App</description>

    <content src="index.html" />

    <plugin name="cordova-plugin-whitelist" />
    <plugin name="cordova-plugin-wifi-manager" />
    <plugin name="cordova-plugin-inappbrowser" />

    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />

    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
